{
    "version": 1,
    "routes": [
        {
            "src": "^/([^.]+)$",
            "dest": "/"
        },
        {
            "src": "^/(.*)\\.(.*)$",
            "dest": "/$1.$2"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}